<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Pcbuilder.Mobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Pcbuilder.Mobile"
    xmlns:views="clr-namespace:Pcbuilder.Mobile.Views"
    Title="Pcbuilder.Mobile">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />


    <ShellContent
        Title="Profile"
        ContentTemplate="{DataTemplate views:ProfilePage}"
        Route="ProfilePage" />

</Shell>
